    "eslint-plugin-react": "^7.37.2",
    "globals": "^15.13.0",
    "package-manager-detector": "^0.2.7",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
